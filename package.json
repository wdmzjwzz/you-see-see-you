    "koa-swig": "^2.2.1",
    "koa2-connect-history-api-fallback": "^0.1.2",
    "lodash": "^4.17.15",
    "lodash-es": "^4.17.15",
    "log4js": "^6.2.1",
    "module-alias": "^2.2.2",
